// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json", /* Recommended React Native TSConfig base */
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "esnext",
    "lib": [
      "es6",
      "ES2019"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "types": [
      "react-native",
      "node",
      "jest"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": false,
    "rootDir": "./",
    "outDir": "./",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": true,
    "noImplicitThis": false,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "downlevelIteration": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@app": [
        "./app"
      ],
      "@app/*": [
        "app/*"
      ],
      "@constants": [
        "app/constants"
      ],
      "@constants/*": [
        "app/constants/*"
      ],
      "@database": [
        "app/database"
      ],
      "@database/*": [
        "app/database/*"
      ],
      "@context": [
        "app/context"
      ],
      "@context/*": [
        "app/context/*"
      ],
      "@hooks": [
        "app/hooks"
      ],
      "@hooks/*": [
        "app/hooks/*"
      ],
      "@initialize": [
        "app/initialize"
      ],
      "@localization": [
        "app/localization"
      ],
      "@localization/*":[
        "app/localization/*"
      ],
      "@managers": [
        "app/managers"
      ],
      "@managers/*": [
        "app/managers/*"
      ],
      "@navigation": [
        "app/navigation"
      ],
      "@navigation/*": [
        "app/navigation/*"
      ],
      "@services": [
        "app/services"
      ],
      "@services/*": [
        "app/services/*"
      ],
      "@screens": [
        "app/screens"
      ],
      "@screens/*": [
        "app/screens/*"
      ],
      "@utils": [
        "app/utils"
      ],
      "@utils/*": [
        "app/utils/*"
      ],
      "@store": [
        "app/store"
      ],
      "@store/*": [
        "app/store/*"
      ],
      "@env": [
        "./env"
      ],
      "@env/*": [
        "env/*"
      ],
      "@typings": [
        "./types"
      ],
      "@assets": [
        "./assets"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@typings/*": [
        "types/*"
      ],
      "*": [
        "./*",
        "node_modules/*"
      ],
    },
  },
  "include": [
    "app/**/*",
    "test/**/*",
    "types/**/*"
  ],
  "exclude": [
    "node_modules",
    "build",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "modules"
  ],
}
